<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="category" catalog="forum" class="Category">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
	<table name="comment" catalog="forum" class="Comment">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="dlb" catalog="forum" class="Dlb">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
	<table name="topic" catalog="forum" class="Topic">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="type" catalog="forum" class="Type">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
<table name="user" catalog="forum" class="User">
	<primary-key>
		<generator class="assigned"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>